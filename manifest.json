{
  "manifest_version": 3,
  "name": "Close Duplicate Tabs",
  "version": "1.0",
  "description": "A Chrome extension to close duplicate tabs.",
  "permissions": [
    "tabs","scripting"
  ],
  "commands":{
    "close": {
      "suggested_key": {
        "windows": "Ctrl+C",
        "linux":"Ctrl+C",
        "chromeos":"Ctrl+C",
        "mac": "MacCtrl+C"
      },
      "description":"Close duplicate tabs"

    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}