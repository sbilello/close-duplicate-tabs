{
  "manifest_version": 3,
  "name": "Close Duplicate Tabs",
  "version": "1.0",
  "description": "A Chrome extension to close duplicate tabs.",
  "permissions": [
    "tabs"
  ],
  "commands":{
    "close": {
      "description":"Close duplicate tabs"
    },
    "_execute_action":{
      "suggested_key":{
        "linux":"Alt+U",
        "windows":"Alt+U",
        "chromeos":"Alt+U",
        "mac":"MacCtrl+U"
      }
    }

  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}